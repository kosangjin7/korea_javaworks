package Choice;

public class IfElseIfEls{
/*
 * @ 학점 계산 프로그램
 * 90~100점:A
 * 80~89점:B
 * 70~79점:C
 * 700점 미만:F 
 * 
 */
	public static void main(String[] args) {
		//다중조건문
		//논리 연산사 &&,||,!
		int score=86;
		char grade=' ' ;
		
		if(score >=90 && score <= 100) {
			
			grade ='A';
		}
		System.out.println("학점은" + grade + "입니다.");

	}

}
